{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "organizationName": {
      "value": "mil"
    },
    "applicationName": {
      "value": "gateway"
    },
    "environmentName": {
      "value": "Development"
    },
    "hostName": {
      "value": "dev"
    },
    "api": {
      "value": {
        "publisherEmail": "api@milochau.com",
        "publisherName": "Antoine Milochau"
      }
    },
    "monitoring": {
      "value": {
        "enableApplicationInsights": true,
        "disableLocalAuth": false,
        "dailyCap": "1",
        "workspaceName": "mil-monitoring-dev-ws",
        "workspaceResourceGroup": "mil-monitoring-dev-rg"
      }
    }
  }
}